setwd("D:\\wangliang\\clinical_statistic\\general_hospital\\routine_pathology_general")


o_11 <- read.csv(file = "routine_pathology_201711.txt", sep = "\t", stringsAsFactors = F)
f_11 <- o_11[-grep("西区", o_11[,7]),]
write.table(f_11, file = "11月总院南区.txt", quote = F, col.names = T, row.names = F,sep = "\t")



